  "version": "0.1",
  "key": "ecommerce",
  "helpers": [
    "Plugins::Ecommerce::EcommerceHelper",
    "Plugins::Ecommerce::EcommerceFunctionsHelper"
  ],
  "options": [
  ],
  "hooks": {
    "on_destroy": [
      "ecommerce_on_destroy"
    ],
    "on_active": [
      "ecommerce_on_active"
    ],
    "on_inactive": [
      "ecommerce_on_inactive"
